{
  "name": "passing-notes-sse",
  "version": "0.0.0",
  "description": "A middleware for sending and receiving server-sent events",
  "keywords": [
    "sse"
  ],
  "homepage": "https://github.com/vinsonchuong/passing-notes-sse",
  "bugs": "https://github.com/vinsonchuong/passing-notes-sse/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/passing-notes-sse",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "type": "module",
  "ava": {
    "verbose": true
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "envs": [
      "browser",
      "node"
    ],
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "packageManager": "yarn@4.0.1",
  "devDependencies": {
    "ava": "^5.3.1",
    "semantic-release": "^22.0.7",
    "xo": "^0.56.0"
  },
  "dependencies": {
    "passing-notes": "^6.8.3"
  }
}
