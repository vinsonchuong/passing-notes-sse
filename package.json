{
  "name": "passing-notes-sse",
  "version": "0.0.0",
  "description": "A middleware for sending and receiving server-sent events",
  "keywords": [
    "sse"
  ],
  "homepage": "https://github.com/vinsonchuong/passing-notes-sse",
  "bugs": "https://github.com/vinsonchuong/passing-notes-sse/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/passing-notes-sse",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "main": "index.js",
  "type": "module",
  "ava": {
    "verbose": true,
    "timeout": "3m"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "envs": [
      "browser",
      "node"
    ],
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "packageManager": "yarn@4.0.2",
  "devDependencies": {
    "ava": "^5.3.1",
    "ava-patterns": "^3.2.1",
    "passing-notes-ui": "^1.3.5",
    "puppet-strings": "^5.0.2",
    "puppet-strings-chrome": "^2.0.4",
    "puppet-strings-open-app": "^4.0.4",
    "semantic-release": "^25.0.0",
    "xo": "^0.60.0"
  },
  "dependencies": {
    "passing-notes": "^6.8.4"
  }
}
